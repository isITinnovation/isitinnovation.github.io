		"@types/styled-components": "^5.1.34",
		"@vercel/node": "^5.1.13",
		"axios": "^1.6.7",
		"bcryptjs": "^2.4.3",
		"crypto-js": "^4.2.0",
		"dotenv": "^16.4.7",
		"jsonwebtoken": "^9.0.2",
		"mysql2": "^3.13.0",
		"react": "^18.2.0",
